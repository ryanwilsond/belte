{
    "files.associations": {
        "*.csproj": "msbuild",
        "vector": "c",
        "xstring": "cpp"
    },
    "files.exclude": {
        "**/bin": true,
        "**/obj": true,
        "**/.vs": true,
    },
    "editor.rulers": [
        80,
        120,
    ],
    "omnisharp.disableMSBuildDiagnosticWarning": true,
    "omnisharp.enableEditorConfigSupport": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.formatOnSave": true,
    "dotnet.defaultSolution": "Belte.sln",
}
